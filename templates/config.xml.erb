<?xml version="1.0" ?>
<config>
    <localeversion suppressWarning="false">0.9.0</localeversion>
    <version>224</version>
    <help url="<%= @help_url %>"/>
    <javaTest url="<%= @fqdn %>/testjava.html" />
    <porttest host="<%= @fqdn %>" application="video/portTest" timeout="10000" />
    <bwMon server="<%= @fqdn %>" application="video/bwTest" />
    <application uri="rtmp://<%= @fqdn %>/bigbluebutton" host="http://<%= @fqdn %>/bigbluebutton/api/enter" />
    <language userSelectionEnabled="true" />
    <skinning enabled="<%= enable_skin %>" url="<%= @theme_skin %>" />
    <shortcutKeys showButton="true" />
    <browserVersions chrome="39" firefox="35" flash="16" java="1.7.0_51" />
    <layout showLogButton="false" showVideoLayout="false" showResetLayout="true" defaultLayout="bbb.layout.name.defaultlayout"
      showToolbar="true" showFooter="true" showMeetingName="true" showHelpButton="true"
      showLogoutWindow="true" showLayoutTools="true" showNetworkMonitor="false" confirmLogout="true"
            showRecordingNotification="true"/>
    <lock allowModeratorLocking="false" disableCamForLockedUsers="false" disableMicForLockedUsers="false" disablePrivateChatForLockedUsers="false"
          disablePublicChatForLockedUsers="false" lockLayoutForLockedUsers="false"/>

  	<modules>
        <module name="ChatModule" url="http://<%= @fqdn %>/client/ChatModule.swf?v=224"
            uri="rtmp://<%= @fqdn %>/bigbluebutton"
      			dependsOn="UsersModule"
            fontSize="12"
            baseTabIndex="701"
            colorPickerIsVisible="false"
            privateEnabled="<%= @private_chat %>"
			      position="top-right"
		    />

		    <module name="UsersModule"
            url="http://<%= @fqdn %>/client/UsersModule.swf?v=224"
            uri="rtmp://<%= @fqdn %>/bigbluebutton"
            allowKickUser="<%= @allow_kick_user %>"
            enableRaiseHand="true"
            enableSettingsButton="true"
            baseTabIndex="301"
		    />

		    <module name="DeskShareModule"
			      url="http://<%= @fqdn %>/client/DeskShareModule.swf?v=224"
            uri="rtmp://<%= @fqdn %>/deskShare"
            showButton="true"
            autoStart="false"
            autoFullScreen="false"
            baseTabIndex="201"
		    />

		    <module name="PhoneModule"
            url="http://<%= @fqdn %>/client/PhoneModule.swf?v=224"
            uri="rtmp://<%= @fqdn %>/sip"
            autoJoin="<%= phone_auto_join %>"
            listenOnlyMode="true"
            presenterShareOnly="false"
            skipCheck="<%= @phone_skip_check %>"
			      showButton="true"
			      enabledEchoCancel="true"
            useWebRTCIfAvailable="true"
            showPhoneOption="false"
            echoTestApp="9196"
			      dependsOn="UsersModule"
		    />

		    <module name="VideoconfModule"
            url="http://<%= @fqdn %>/client/VideoconfModule.swf?v=224"
            uri="rtmp://<%= @fqdn %>/video"
			      dependson = "UsersModule"
            videoQuality = "<%= @video_quality %>"
            presenterShareOnly = "<%= @presenter_share_only %>"
            controlsForPresenter = "false"
            resolutions = "<%= @resolutions %>"
			      autoStart = "false"
            skipCamSettingsCheck = "false"
			      showButton = "true"
			      showCloseButton = "true"
			      publishWindowVisible = "true"
			      viewerWindowMaxed = "false"
			      viewerWindowLocation = "top"
			      camKeyFrameInterval = "30"
            camModeFps = "<%= @cam_mode_fps %>"
            camQualityBandwidth = "<%= @cam_quality_bandwith %>"
            camQualityPicture = "<%= @cam_quality_picture %>"
            smoothVideo = "false"
            applyConvolutionFilter="-1, 0, -1, 0, 6, 0, -1, 0, -1"
            filterBias="0"
            filterDivisor="4"
            enableH264 = "true"
            h264Level = "2.1"
            h264Profile = "main"
            displayAvatar = "false"
            focusTalking = "false"
            glowColor = "0x4A931D"
            glowBlurSize = "30.0"
		    />

		    <module name="WhiteboardModule"
            url="http://<%= @fqdn %>/client/WhiteboardModule.swf?v=4105"
            uri="rtmp://<%= @fqdn %>/bigbluebutton"
			      dependsOn="PresentModule"
            baseTabIndex = "601"
            whiteBoardAccess = "presenter"
            keepToolbarVisible = "false"
		    />

        <module name="PollingModule"
          url="http://<%= @fqdn %>/client/PollingModule.swf?v=224"
          uri="rtmp://<%= @fqdn %>/bigbluebutton"
          dependsOn="PresentModule"
        />


		    <module name="PresentModule"
            url="http://<%= @fqdn %>/client/PresentModule.swf?v=224"
            uri="rtmp://<%= @fqdn %>/bigbluebutton"
            host="http://<%= @fqdn %>"
			      showPresentWindow="true"
			      showWindowControls="true"
            openExternalFileUploadDialog="false"
			      dependsOn="UsersModule"
            baseTabIndex = "501"
            maxFileSize = "30"
		    />

		    <module name="VideodockModule"
            url="http://<%= @fqdn %>/client/VideodockModule.swf?v=224"
            uri="rtmp://<%= @fqdn %>/bigbluebutton"
			      dependsOn="VideoconfModule, UsersModule"
			      autoDock="true"
            showControls = "true"
			      maximizeWindow="false"
			      position="bottom-right"
			      width="172"
			      height="179"
			      layout="smart"
			      oneAlwaysBigger="false"
            baseTabIndex="401"
		    />

        <module name="LayoutModule"
          url="http://<%= @fqdn %>/client/LayoutModule.swf?v=224"
          uri="rtmp://<%= @fqdn %>/bigbluebutton"
          layoutConfig="http://<%= @fqdn %>/client/conf/layout.xml"
          enableEdit="false"
        />

	</modules>
</config>
